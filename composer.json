{
    "name": "hexlet/code",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/index.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/collections": "<11",
        "nesbot/carbon": "<3",
        "symfony/string": "<6"
    },
    "require-dev": {
        "psy/psysh": "@stable",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/extension-installer": "^1.4",
        "symfony/var-dumper": "<7",
        "phpunit/phpunit": "<11",
        "squizlabs/php_codesniffer": "*",
        "phpstan/phpstan": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
